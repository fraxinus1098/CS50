{
    "manifest_version": 3,
    "name": "Claude Prompting Tips",
    "version": "1.0.0",
    "description": "Basic official prompting tips for Claude",
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "permissions": [
      "activeTab",
      "sidePanel"
    ],
    "action": {
        "default_title": "Claude Prompting Tips"
    },
    "host_permissions": [
      "https://claude.ai/*"
    ]
}